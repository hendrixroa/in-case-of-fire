{
  "name": "in-case-of-fire",
  "version": "1.0.0",
  "description": "A minimal Electron application for app scheduler git",
  "main": "main.js",
  "scripts": {
    "start": "npm run compile && electron main.js",
    "compile": "browserify -t [ babelify --presets [ react es2015 ] ] src/app.js -o build/js/app.js",
    "dist": "build -l deb --ia32 -p onTag",
    "package": "npm run compile && electron-packager . --overwrite --platform=linux --arch=ia32  --prune=false --out=dist",
    "deb": "npm run package && electron-installer-debian --src dist/in-case-of-fire-linux-ia32/ --arch i386 --config config.json"
  },
  "repository": "https://github.com/hendrixroa/in-case-of-fire",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "git ",
    "emergency"
  ],
  "author": {
    "name": "Hendrix Roa",
    "email": "hendrix.alex87@gmail.com",
    "url": "https://hendrixroa.github.io"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "electron": "~1.6.2",
    "electron-builder": "^19.19.1",
    "electron-installer-debian": "^0.5.2",
    "electron-json-storage": "^3.0.7",
    "electron-packager": "^8.7.2",
    "simple-git": "^1.75.0"
  },
  "dependencies": {
    "material-auto-rotating-carousel": "^1.4.2",
    "react": "^0.14.8",
    "react-dom": "^0.14.8"
  }
}
